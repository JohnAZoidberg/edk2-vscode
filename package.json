{
	"name": "edk2-vscode",
	"displayName": "edk2-vscode",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:edk2_dsc",
		"onLanguage:edk2_dec"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "edk2_dsc",
				"extensions": [
					".dsc"
				]
			},
			{
				"id": "edk2_dec",
				"extensions": [
					".dec"
				]
			}
		],
		"grammars": [
			{
				"language": "edk2_dsc",
				"scopeName": "source.edk2_dsc",
				"path": "./syntaxes/edk2_dsc.tmLanguage.json"
			},
			{
				"language": "edk2_dec",
				"scopeName": "source.edk2_dec",
				"path": "./syntaxes/edk2_dec.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
