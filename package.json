{
	"name": "edk2-vscode",
	"displayName": "edk2-vscode",
	"description": "",
	"version": "1.0.1",
	"publisher": "walonli",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:edk2_fdf",
		"onLanguage:edk2_dsc",
		"onLanguage:edk2_dec",
		"onLanguage:edk2_inf",
		"onLanguage:edk2_vfr"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "edk2_dsc",
				"extensions": [
					".dsc"
				]
			},
			{
				"id": "edk2_dec",
				"extensions": [
					".dec"
				]
			},
			{
				"id": "edk2_fdf",
				"extensions": [
					".fdf"
				]
			},
			{
				"id": "edk2_inf",
				"extensions": [
					".inf"
				]
			},
			{
				"id": "edk2_uni",
				"extensions": [
					".uni"
				]
			},
			{
				"id": "edk2_vfr",
				"extensions": [
					".vfr",
					".hfr"
				]
			}
		],
		"grammars": [
			{
				"language": "edk2_dsc",
				"scopeName": "source.edk2_dsc",
				"path": "./syntaxes/edk2_dsc.tmLanguage.json"
			},
			{
				"language": "edk2_dec",
				"scopeName": "source.edk2_dec",
				"path": "./syntaxes/edk2_dec.tmLanguage.json"
			},
			{
				"language": "edk2_fdf",
				"scopeName": "source.edk2_fdf",
				"path": "./syntaxes/edk2_fdf.tmLanguage.json"
			},
			{
				"language": "edk2_inf",
				"scopeName": "source.edk2_inf",
				"path": "./syntaxes/edk2_inf.tmLanguage.json"
			},
			{
				"language": "edk2_uni",
				"scopeName": "source.edk2_uni",
				"path": "./syntaxes/edk2_uni.tmLanguage.json"
			},
			{
				"language": "edk2_vfr",
				"scopeName": "source.edk2_vfr",
				"path": "./syntaxes/edk2_vfr.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/WalonLi/edk2-vscode.git"
	},
	"homepage": "https://github.com/WalonLi/edk2-vscode/blob/master/README.md"
}
